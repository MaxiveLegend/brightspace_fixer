{
  "name": "Brightspace layout fixer",
  "description": "A plugin that puts the focus on the active courses on brightspace pages",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "images/icon-16.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/script.js"],
      "matches": [
        "*://brightspace.avans.nl/*",
        "*://*.brightspace.nl/*",
        "*://*.brightspace.com/*"
      ]
    }
  ]
}
